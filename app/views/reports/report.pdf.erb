<!DOCTYPE html>
<html>
<body>
<h3>Projects list</h3>
<% if(@projects) %>
	<ul>
	<% @projects.each do |p| %>
		<li><%= p.name %> | <%= p.start_date %> | <%= p.end_date %></li>	
	<% end %>
	</ul>
<% end if %>
<div>
	<img width="400" class="figure" src="data:image/svg+xml;base64," ></img>
</div>
</body>
</html>




