{{#if donors}}
  <h2>Donors list</h2>
  <table>
    <tr>
      <th>Donor</th>
      <th>Projects</th>
      <th>Countries</th>
      <th>Sectors</th>
    </tr>
    {{#each donors}}
      <tr>
        <td>{{name}}</td>
        {{#if projectsCount}}<td>{{projectsCount}}</td>{{/if}}
        {{#if countriesCount}}<td>{{countriesCount}}</td>{{/if}}
        {{#if sectorsCount}}<td>{{sectorsCount}}</td>{{/if}}
      </tr>
    {{/each}}
  </table>
{{/if}}


