{{#if projects}}
  <h2>Projects list <button class="is-inline-btn is-unprintable">Close</button></h2>
  {{#if title}}<p>Top {{title}}</p>{{/if}}
  <ul class="mod-report-lists-selector">
    <li>
      <span class="current">Sort by number of donors</span>
      <ul>
        <li><a href="#organizations" data-category="organizationName">Sort by organization name</a></li>
        <li><a href="#donors" data-category="donorsCount">Sort by number of donors</a></li>
        <li><a href="#countries" data-category="countriesCount">Sort by number of countries</a></li>
        <li><a href="#sectors" data-category="sectorsCount">Sort by number of sectors</a></li>
      </ul>
    </li>
  </ul>
  <table>
    <tr>
      <th>Project</th>
      <th>Donors</th>
      <th>Countries</th>
      <th>Sectors</th>
    </tr>
    {{#each projects}}
      <tr>
        <td class="mod-report-lists-project">{{#if organizationName}}<strong>{{organizationName}}</strong><br>{{/if}}{{name}}</td>
        {{#if donorsCount}}<td>{{donorsCount}}</td>{{else}}<td></td>{{/if}}
        {{#if countriesCount}}<td>{{countriesCount}}</td>{{else}}<td></td>{{/if}}
        {{#if sectorsCount}}<td>{{sectorsCount}}</td>{{else}}<td></td>{{/if}}
      </tr>
    {{/each}}
  </table>
{{/if}}

