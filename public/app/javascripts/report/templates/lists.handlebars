{{#if donors}}
  <h2>Donors list <button class="is-inline-btn is-unprintable">Close</button></h2>
  <ul class="mod-report-lists-selector">
    <li>
      <span class="current">Sort by number of projects</span>
      <ul>
        <li><a href="#projects" data-category="projectsCount">Sort by number of projects</a></li>
        <li><a href="#countries" data-category="countriesCount">Sort by number of countries</a></li>
        <li><a href="#sectors" data-category="sectorsCount">Sort by number of sectors</a></li>
      </ul>
    </li>
  </ul>
  <table>
    <tr>
      <th>Donor</th>
      <th>Projects</th>
      <th>Countries</th>
      <th>Sectors</th>
    </tr>
    {{#each donors}}
      <tr>
        <td>{{name}}</td>
        {{#if projectsCount}}<td>{{projectsCount}}</td>{{else}}<td></td>{{/if}}
        {{#if countriesCount}}<td>{{countriesCount}}</td>{{else}}<td></td>{{/if}}
        {{#if sectorsCount}}<td>{{sectorsCount}}</td>{{else}}<td></td>{{/if}}
      </tr>
    {{/each}}
  </table>
{{/if}}


